<div class="jumbotron text-center">
      <div class="overlay">
      </div>
      <div class="background-image" style="background-image:url('newspaper.jpg')">
      </div>
      <div class="caption">
        <h1>Mongo Scraper</h1>
        <p>New York Times Edition</p>
      </div>
    </div>



<div class="row">
    
    <div>
        {{#each article}}
        <div class="card-panel">
            <h4 style="max-width:780px">{{this.title}}</h4>
            <br>
            <a href="https://www.nytimes.com{{this.link}}" class="waves-effect waves-light btn float-right" id="btnSignUp">Read Article</a>
                    <br>            <br>

        </div>
        {{/each}}
    </div>
</div>




